@model List<DataEntities.Therapist>
@if ((Model!=null) && (Model.Count>0)) {
<table class="table">
    <thead>
        <tr>
            <th scope="col">Therapist</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var t in Model)
        {
        <tr>
            <td>@t.firstName @t.lastName @t.middleName</td>
        </tr>
        }
        </tbody>
    </table>
    }
