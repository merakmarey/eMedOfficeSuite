            <div class="col">
                <label for="referringPhysicianNPI">Referring Physician NPI</label>
                <input type="text" class="form-control" id="referringPhysicianNPI" name="referringPhysicianNPI">
            </div>
        </div>
        <div class="row">
            <div class="col">
                <label for="referringProviderMedicaid">Referring Provider Medicaid</label>
                <input type="text" class="form-control" id="referringProviderMedicaid" name="referringProviderMedicaid">
            </div>
            <div class="col">
                <label for="referringPhysicianFirstName">Referring Physician First Name</label>
                <input type="text" class="form-control" id="referringPhysicianFirstName" name="referringPhysicianFirstName">
            </div>
        </div>
        <div class="row">
            <div class="col">
                <label for="referringPhysicianMiddleName">Referring Physician Middle Name</label>
                <input type="text" class="form-control" id="referringPhysicianMiddleName" name="referringPhysicianMiddleName">
            </div>
            <div class="col">
                <label for="referringPhysicianLastName">Referring Physician Last Name</label>
                <input type="text" class="form-control" id="referringPhysicianLastName" name="referringPhysicianLastName">
            </div>
        </div>
        <div class="row">
            <div class="col">
                <label for="referringPhysicianPhone">Referring Physician Phone</label>
                <input type="text" class="form-control" id="referringPhysicianPhone" name="referringPhysicianPhone">
            </div>
            <div class="col">
                <label for="referringPhysicianEmail">Referring Physician Email</label>
                <input type="email" class="form-control" id="referringPhysicianEmail" name="referringPhysicianEmail">
            </div>
        </div>
        <div class="row">
            <div class="col">
                <label for="referalExpirationDate">Referal Expiration Date</label>
                <input type="date" class="form-control" id="referalExpirationDate" name="referalExpirationDate">
            </div>
            <div class="col">
                <label for="mdLicenseNumber">MD License Number</label>
                <input type="text" class="form-control" id="mdLicenseNumber" name="mdLicenseNumber">
            </div>
        </div>
        </div>
        <div id="SerCo" class="tab-pane">
        <div class="row">
            <div class="col">
                <label for="serviceLocationId">Service Location</label>
                <select class="form-select" id="serviceLocationId" name="serviceLocationId">
                    @foreach (var tt in Model.serviceLocationTypes)
                    {
                        <option value="@tt.Key">@tt.Value</option>
                    }
                </select>
            </div>
            <div class="col">
                <label for="startOfServiceDate">Start Of Service Date</label>
                <input type="date" class="form-control" id="startOfServiceDate" name="startOfServiceDate">
            </div>
        </div>
        <div class="row">
            <div class="col">
                <label for="initialAssesmentDate">Initial Assesment Date</label>
                <input type="date" class="form-control" id="initialAssesmentDate" name="initialAssesmentDate">
            </div>
            <div class="col">
                <label for="initialBASPDate">Initial B A S P Date</label>
                <input type="date" class="form-control" id="initialBASPDate" name="initialBASPDate">
            </div>
        </div>
        <div class="row">
            <div class="col">
                <label for="coordinatorApprovalDate">Coordinator Approval Date</label>
                <input type="date" class="form-control" id="coordinatorApprovalDate" name="coordinatorApprovalDate">
            </div>
            <div class="col">
                <label for="cordinatorEmail">Cordinator Email</label>
                <input type="email" class="form-control" id="cordinatorEmail" name="cordinatorEmail">
            </div>
        </div>
        <div class="row">
            <div class="col">
