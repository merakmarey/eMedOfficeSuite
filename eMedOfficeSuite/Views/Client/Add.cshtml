@model dynamic

@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()

<ul class="nav nav-tabs mb-4" id="myTab" role="tablist">
    <li class="nav-item" role="presentation">
        <button class="nav-link active" id="Client-tab" data-bs-toggle="tab" data-bs-target="#Client" type="button" role="tab" aria-controls="Client" aria-selected="true">Client</button>
    </li>
    <li class="nav-item" role="presentation">
        <button class="nav-link" id="Primary-tab" data-bs-toggle="tab" data-bs-target="#Primary" type="button" role="tab" aria-controls="Primary" aria-selected="false">Primary Insured</button>
    </li>
    <li class="nav-item" role="presentation">
        <button class="nav-link" id="Secondary-tab" data-bs-toggle="tab" data-bs-target="#Secondary" type="button" role="tab" aria-controls="Secondary" aria-selected="false">Secondary Insured</button>
    </li>
    <li class="nav-item" role="presentation">
        <button class="nav-link" id="DiagRef-tab" data-bs-toggle="tab" data-bs-target="#DiagRef" type="button" role="tab" aria-controls="DiagRef" aria-selected="false">Diagnostic and Referal</button>
    </li>
    <li class="nav-item" role="presentation">
        <button class="nav-link" id="SerCo-tab" data-bs-toggle="tab" data-bs-target="#SerCo" type="button" role="tab" aria-controls="SerCo" aria-selected="false">Service and Coordination</button>
    </li>
</ul>

    <div class="tab-content" id="myTabContent">
        <div class="tab-pane show active" id="Client" role="tabpanel" aria-labelledby="Client-tab">
            <div class="row">
                <div class="col">
                    <label for="directorId">Director</label>
                    <select class="form-select" id="directorId" name="directorId">
                        @foreach (var tt in Model.directorTypes)
                        {
                            <option value="@tt.Key">@tt.Value</option>
                        }
                    </select>
                </div>
                <div class="col">
                    <label for="email">Email</label>
                    <input type="email" class="form-control" id="email" name="email">
                </div>
            </div>
            <div class="row">
                <div class="col">
                    <label for="phone">Phone</label>
                    <input type="text" class="form-control" id="phone" name="phone">
                </div>
                <div class="col">
                    <label for="birthdate">Birthdate</label>
                    <input type="date" class="form-control" id="birthdate" name="birthdate">
                </div>
            </div>
            <div class="row">
                <div class="col">
                    <label for="firstName">First Name</label>
                    <input type="text" class="form-control" id="firstName" name="firstName">
                </div>
                <div class="col">
                    <label for="middleName">Middle Name</label>
                    <input type="text" class="form-control" id="middleName" name="middleName">
                </div>
            </div>
            <div class="row">
                <div class="col">
                    <label for="lastName">Last Name</label>
                    <input type="text" class="form-control" id="lastName" name="lastName">
                </div>
                <div class="col">
                    <label for="genderId">Gender</label>
                    <select class="form-select" id="genderId" name="genderId">
                        @foreach (var tt in Model.genderTypes)
                        {
                            <option value="@tt.Key">@tt.Value</option>
                        }
                    </select>
                </div>
            </div>
            <div class="row">
                <div class="col">
                    <label for="ssn">Ssn</label>
                    <input type="text" class="form-control" id="ssn" name="ssn">
                </div>
                <div class="col">
                    <label for="address1">Address1</label>
                    <input type="text" class="form-control" id="address1" name="address1">
                </div>
            </div>
            <div class="row">
                <div class="col">
                    <label for="address2">Address2</label>
                    <input type="text" class="form-control" id="address2" name="address2">
                </div>
                <div class="col">
                    <label for="cityId">City</label>
                    <select class="form-select" id="cityId" name="cityId">
                        @foreach (var tt in Model.cityTypes)
                        {
                            <option value="@tt.Key">@tt.Value</option>
                        }
                    </select>
                </div>
            </div>
            <div class="row">
                <div class="col">
                    <label for="stateId">State</label>
                    <select class="form-select" id="stateId" name="stateId">
                        @foreach (var tt in Model.stateTypes)
                        {
                            <option value="@tt.Key">@tt.Value</option>
                        }
                    </select>
                </div>
                <div class="col">
                    <label for="zip">Zip</label>
                    <input type="text" class="form-control" id="zip" name="zip">
                </div>
            </div>
        </div>
        <div class="tab-pane" id="Primary" role="tabpanel" aria-labelledby="Primary-tab">
            <div class="row">
                <div class="col">
                    <label for="primaryPayerId">Primary Payer</label>
                    <div class="checkbox-inline">
                        @foreach (var payer in Model.payerTypes)
                        {
                            <input type="checkbox" class="form-check-input" name="primaryPayerId" value="@payer.Key">
                            <label class="form-check-label" for="en">@payer.Value</label>
                        }
                    </div>
                </div>
                <div class="col">
                    <label for="InsuraceCopayment">Insurace Copayment</label>
                    <input type="number" min="0" step="0.01" class="form-control" id="InsuraceCopayment" name="InsuraceCopayment">
                </div>
            </div>
            <div class="row">
                <div class="col">
                    <label for="relationshipToPatientId">Relationship To Patient</label>
                    <select class="form-select" id="relationshipToPatientId" name="relationshipToPatientId">
                        @foreach (var tt in Model.relationshipToPatientTypes)
                        {
                            <option value="@tt.Key">@tt.Value</option>
                        }
                    </select>
                </div>
                <div class="col">
                    <label for="insuranceId">Insurance</label>
                    <input type="text" class="form-control" id="insuranceId" name="insuranceId">
                </div>
            </div>
            <div class="row">
                <div class="col">
                    <label for="insuredFirstName">Insured First Name</label>
                    <input type="text" class="form-control" id="insuredFirstName" name="insuredFirstName">
                </div>
                <div class="col">
                    <label for="insuredMiddleName">Insured Middle Name</label>
                    <input type="text" class="form-control" id="insuredMiddleName" name="insuredMiddleName">
                </div>
            </div>
            <div class="row">
                <div class="col">
                    <label for="insuredLastName">Insured Last Name</label>
                    <input type="text" class="form-control" id="insuredLastName" name="insuredLastName">
                </div>
                <div class="col">
                    <label for="primaryInsuredGender">Primary Insured Gender</label>
                    <input type="text" class="form-control" id="primaryInsuredGender" name="primaryInsuredGender">
                </div>
            </div>
            <div class="row">
                <div class="col">
                    <label for="insuredAddress1">Insured Address1</label>
                    <input type="text" class="form-control" id="insuredAddress1" name="insuredAddress1">
                </div>
                <div class="col">
                    <label for="insuredAddress2">Insured Address2</label>
                    <input type="text" class="form-control" id="insuredAddress2" name="insuredAddress2">
                </div>
            </div>
            <div class="row">
                <div class="col">
                    <label for="insuredCityId">Insured City</label>
                    <select class="form-select" id="insuredCityId" name="insuredCityId">
                        @foreach (var tt in Model.cityTypes)
                        {
                            <option value="@tt.Key">@tt.Value</option>
                        }
                    </select>
                </div>
                <div class="col">
                    <label for="insuredStateId">Insured State</label>
                    <select class="form-select" id="insuredStateId" name="insuredStateId">
                        @foreach (var tt in Model.stateTypes)
                        {
                            <option value="@tt.Key">@tt.Value</option>
                        }
                    </select>
                </div>
            </div>
            <div class="row">
                <div class="col">
                    <label for="insuredZip">Insured Zip</label>
                    <input type="text" class="form-control" id="insuredZip" name="insuredZip">
                </div>
                <div class="col">
                    <label for="primaryInsuredBirthdate">Primary Insured Birthdate</label>
                    <input type="date" class="form-control" id="primaryInsuredBirthdate" name="primaryInsuredBirthdate">
                </div>
            </div>
            <div class="row">
                <div class="col">
                    <label for="insuredPhone">Insured Phone</label>
                    <input type="text" class="form-control" id="insuredPhone" name="insuredPhone">
                </div>
                <div class="col">
                </div>
            </div>
        </div>
        <div class="tab-pane" id="Secondary" role="tabpanel" aria-labelledby="Secondary-tab">
            <div class="row">
                <div class="col">
                    <label for="secondaryPayerId">Secondary Payer</label>
                    <div class="checkbox-inline">
                        @foreach (var payer in Model.payerTypes)
                        {
                            <input type="checkbox" class="form-check-input" name="secondaryPayerId" value="@payer.Key">
                            <label class="form-check-label" for="en">@payer.Value</label>
                        }
                    </div>
                </div>
                <div class="col">
                </div>
            </div>
            <div class="row">
                <div class="col">
                    <label for="secondaryInsuranceId">Secondary Insurance</label>
                    <input type="text" class="form-control" id="secondaryInsuranceId" name="secondaryInsuranceId">
                </div>
                <div class="col">
                    <label for="secondaryCopayment">Secondary Copayment</label>
                    <input type="number" min="0" step="0.01" class="form-control" id="secondaryCopayment" name="secondaryCopayment">
                </div>
            </div>
            <div class="row">
                <div class="col">
                    <label for="secondaryRelatioshipToPatient">Secondary Relatioship To Patient</label>
                    <input type="text" class="form-control" id="secondaryRelatioshipToPatient" name="secondaryRelatioshipToPatient">
