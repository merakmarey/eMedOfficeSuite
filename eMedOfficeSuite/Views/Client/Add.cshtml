                <div class="col">
                    <label for="secondaryFirstName">Secondary First Name</label>
                    <input type="text" class="form-control" id="secondaryFirstName" name="secondaryFirstName">
                </div>
            </div>
            <div class="row">
                <div class="col">
                    <label for="secondaryMiddleName">Secondary Middle Name</label>
                    <input type="text" class="form-control" id="secondaryMiddleName" name="secondaryMiddleName">
                </div>
                <div class="col">
                    <label for="secondayLastName">Seconday Last Name</label>
                    <input type="text" class="form-control" id="secondayLastName" name="secondayLastName">
                </div>
            </div>
            <div class="row">
                <div class="col">
                    <label for="secondaryInsuredGender">Secondary Insured Gender</label>
                    <input type="text" class="form-control" id="secondaryInsuredGender" name="secondaryInsuredGender">
                </div>
                <div class="col">
                    <label for="secondaryInsuredAddress1">Secondary Insured Address1</label>
                    <input type="text" class="form-control" id="secondaryInsuredAddress1" name="secondaryInsuredAddress1">
                </div>
            </div>
            <div class="row">
                <div class="col">
                    <label for="secondaryInsuredAddress2">Secondary Insured Address2</label>
                    <input type="text" class="form-control" id="secondaryInsuredAddress2" name="secondaryInsuredAddress2">
                </div>
                <div class="col">
                    <label for="secondaryInsuredCityId">Secondary Insured City</label>
                    <select class="form-select" id="secondaryInsuredCityId" name="secondaryInsuredCityId">
                        @foreach (var tt in Model.cityTypes)
                        {
                            <option value="@tt.Key">@tt.Value</option>
                        }
                    </select>
                </div>
            </div>
            <div class="row">
                <div class="col">
                    <label for="secondaryInsuredStateId">Secondary Insured State</label>
                    <select class="form-select" id="secondaryInsuredStateId" name="secondaryInsuredStateId">
                        @foreach (var tt in Model.stateTypes)
                        {
                            <option value="@tt.Key">@tt.Value</option>
                        }
                    </select>
                </div>
                <div class="col">
                    <label for="secondaryInsuredZip">Secondary Insured Zip</label>
                    <input type="text" class="form-control" id="secondaryInsuredZip" name="secondaryInsuredZip">
                </div>
            </div>
            <div class="row">
                <div class="col">
                    <label for="secondaryInsuredBirthdate">Secondary Insured Birthdate</label>
                    <input type="date" class="form-control" id="secondaryInsuredBirthdate" name="secondaryInsuredBirthdate">
                </div>
                <div class="col">
                    <label for="secondaryInsuredPhone">Secondary Insured Phone</label>
                    <input type="text" class="form-control" id="secondaryInsuredPhone" name="secondaryInsuredPhone">
                </div>
            </div>
        </div>
        <div id="DiagRef" class="tab-pane">
        <div class="row">
            <div class="col">
                <label for="clientDiagnosedCodes">Client Diagnosed Codes</label>
                <input type="text" class="form-control" id="clientDiagnosedCodes" name="clientDiagnosedCodes">
